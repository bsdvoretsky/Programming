(define (count-of-strings file)
  (let ((counter 0) (char 0)
                    (port (open-input-file file)))
    (define (loop counter char)
      (let ((symbol (read-char port)))
       
        (if (and (equal? symbol #\newline)
                 (not (= char 0)))
            (begin
              (set! counter (+ 1 counter)) (set! char 0))
                 
            (if (not (equal? symbol #\newline))
                (if (eof-object? symbol)
                    (set! counter (+ counter 1))
                    (loop counter (+ char 1))
                    ))
            )))
    (begin
      (loop counter char)
      (write counter)
      (close-input-port port))))